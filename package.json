{
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-toggle": "^1.1.2",
    "lucide-react": "^0.482.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@playwright/test": "^1.51.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^3.0.8",
    "@vitest/ui": "^3.0.8",
    "abcjs": "^6.1.9",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.3.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.26.1",
    "vite": "^5.2.13",
    "vitest": "^3.0.8"
  },
  "lint-staged": {
    "**/*.ts": "prettier --write --ignore-unknown",
    "**/*.tsx": "prettier --write --ignore-unknown"
  },
  "name": "harrisapp",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json && vite build",
    "test:all": "vitest run --config vitest.config.ts --exclude \"E2E/**\" --exclude \"**/*.perf.test.{ts,tsx}\" --exclude \"**/*.visual.test.{ts,tsx}\"",
    "test:coverage": "vitest run --coverage --config vitest.config.ts --exclude \"E2E/**\" --exclude \"**/*.perf.test.{ts,tsx}\" --exclude \"**/*.visual.test.{ts,tsx}\"",
    "test:coverage:html": "vitest run --coverage --config vitest.config.ts --exclude \"E2E/**\" --exclude \"**/*.perf.test.{ts,tsx}\" --exclude \"**/*.visual.test.{ts,tsx}\" --reporter=html",
    "test:coverage:open": "npm run test:coverage:html && npx vite preview --outDir coverage",
    "test:e2e": "playwright test --project=\"E2E Tests\" --config=playwright.config.ts",
    "test:visual": "playwright test --project=\"Visual Tests\" --config=playwright.config.ts",
    "test:perf": "playwright test --project=\"Performance Tests\" --config=playwright.config.ts",
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=18.18.0"
  }
}
