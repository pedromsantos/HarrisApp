{
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle": "^1.1.9",
    "abcjs": "^6.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/vite": "^4.1.8",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitejs/plugin-react-swc": "^3.10.1",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/ui": "^3.0.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vitest": "^3.0.8"
  },
  "lint-staged": {
    "**/*.ts": "prettier --write --ignore-unknown",
    "**/*.tsx": "prettier --write --ignore-unknown"
  },
  "name": "harrisapp",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json && vite build",
    "test:all": "vitest run --config vitest.config.ts --exclude \"E2E/**\" --exclude \"**/*.perf.test.{ts,tsx}\" --exclude \"**/*.visual.test.{ts,tsx}\"",
    "test:coverage": "vitest run --coverage --config vitest.config.ts --exclude \"E2E/**\" --exclude \"**/*.perf.test.{ts,tsx}\" --exclude \"**/*.visual.test.{ts,tsx}\"",
    "test:coverage:html": "vitest run --coverage --config vitest.config.ts --exclude \"E2E/**\" --exclude \"**/*.perf.test.{ts,tsx}\" --exclude \"**/*.visual.test.{ts,tsx}\" --reporter=html",
    "test:coverage:open": "npm run test:coverage:html && npx vite preview --outDir coverage",
    "test:e2e": "playwright test --project=\"E2E Tests\" --config=playwright.config.ts",
    "test:visual": "playwright test --project=\"Visual Tests\" --config=playwright.config.ts",
    "test:visual:update": "playwright test --project=\"Visual Tests\" --config=playwright.config.ts --update-snapshots",
    "test:perf": "playwright test --project=\"Performance Tests\" --config=playwright.config.ts",
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview"
  },
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=18.18.0"
  }
}
