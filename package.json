{
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle": "^1.1.10",
    "abcjs": "^6.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.2.3",
    "react-piano": "^3.1.3",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "5.1.2",
    "@vitejs/plugin-react-swc": "4.2.2",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1",
    "vitest": "4.0.16"
  },
  "lint-staged": {
    "**/*.ts": "prettier --write --ignore-unknown",
    "**/*.tsx": "prettier --write --ignore-unknown"
  },
  "name": "harrisapp",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json && vite build && npm run lint && npm run test:all",
    "test:all": "vitest run --reporter=verbose --no-file-parallelism --run",
    "test:coverage": "vitest run --coverage --config vitest.config.ts --exclude \"E2E/**\" --exclude \"**/*.perf.test.{ts,tsx}\" --exclude \"**/*.visual.test.{ts,tsx}\"",
    "test:coverage:html": "vitest run --coverage --config vitest.config.ts --exclude \"E2E/**\" --exclude \"**/*.perf.test.{ts,tsx}\" --exclude \"**/*.visual.test.{ts,tsx}\" --reporter=html",
    "test:coverage:open": "npm run test:coverage:html && npx vite preview --outDir coverage",
    "test:e2e": "playwright test --project=\"E2E Tests\" --config=playwright.config.ts",
    "test:visual": "playwright test --project=\"Visual Tests\" --config=playwright.config.ts",
    "test:visual:update": "playwright test --project=\"Visual Tests\" --config=playwright.config.ts --update-snapshots",
    "test:perf": "playwright test --project=\"Performance Tests\" --config=playwright.config.ts",
    "dev:front": "vite",
    "dev:backend": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:front\"",
    "server:install": "cd server && npm install",
    "server:build": "cd server && npm run build",
    "server:start": "cd server && npm start",
    "worker:dev": "cd server && npm run dev:worker",
    "worker:deploy": "cd server && npm run deploy:worker",
    "worker:deploy:prod": "cd server && npm run deploy:worker:prod",
    "worker:secret": "cd server && npm run secret:put",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview"
  },
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=22.16.0"
  }
}
