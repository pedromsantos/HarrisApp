{
  "collectCoverageFrom": ["src/**/*.ts", "src/**/*.tsx", "!src/__test__/**/*"],
  "coverageReporters": ["json", "lcov", "text", "html"],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 95,
      "lines": 95,
      "statements": 95
    }
  },
  "preset": "ts-jest",
  "testTimeout": 10000,
  "testEnvironment": "jsdom",
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  }
}
